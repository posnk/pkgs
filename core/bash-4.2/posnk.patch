-
-	      temp = savestring (timebuf);
-	      goto add_string;
-
-	    case 'D':		/* strftime format */
-	      if (string[1] != '{')		/* } */
-		goto not_escape;
-
-	      (void) time (&the_time);
-	      tm = localtime (&the_time);
-	      string += 2;			/* skip { */
-	      timefmt = xmalloc (strlen (string) + 3);
-	      for (t = timefmt; *string && *string != '}'; )
-		*t++ = *string++;
-	      *t = '\0';
-	      c = *string;	/* tested at add_string */
-	      if (timefmt[0] == '\0')
