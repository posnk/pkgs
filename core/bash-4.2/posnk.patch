-xparse_dolparen (base, string, indp, flags)
-     char *base;
-     char *string;
-     int *indp;
-     int flags;
-{
-  sh_parser_state_t ps;
-  int orig_ind, nc, sflags;
-  char *ret, *s, *ep, *ostring;
-
-  /*yydebug = 1;*/
-  orig_ind = *indp;
