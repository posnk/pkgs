-      if (posixly_correct && c == '!')
-	{
-	  if (*string == '!')
-	    {
-	      temp = savestring ("!");
-	      goto add_string;
-	    }
-	  else
-	    {
-#if !defined (HISTORY)
-		temp = savestring ("1");
-#else /* HISTORY */
-		temp = itos (history_number ());
-#endif /* HISTORY */
-		string--;	/* add_string increments string again. */
-		goto add_string;
-	    }
-	}
-      if (c == '\\')
-	{
-	  c = *string;
-
-	  switch (c)
